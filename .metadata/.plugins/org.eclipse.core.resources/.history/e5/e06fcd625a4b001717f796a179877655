import java.util.Arrays;

public class Task2 {
	public static void main(String[] args) {
		int[] arr1 = { 2, 3, 4 };
		int[] arr2 = arr1;
		int[] arr3 = { 2, 3, 4 };
		arr2[2] = 140;

		System.out.println(arr1);
		System.out.println(arr2);
		System.out.println(arr3);
		System.out.println(Arrays.equals(arr1, arr3));

	}
}
