public class Task2 {
	public static void main(String[] args) {
		int[] arr1 = { 2, 3, 4 };
		int[] arr3 = { 2, 5, 4 };

		boolean areEqual = true;

		if (arr1.length != arr3.length) {
			areEqual = false;
			return;
		}

		for (int i = 0; i < arr1.length; i++) {
			if (arr1[i] == arr3[i]) {
				areEqual = true;
			} else {
				areEqual = false;
				break;
			}
		}

		System.out.println(areEqual ? "Equal" : "Not Equal");
	}
}
